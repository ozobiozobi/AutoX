{
    "compilerOptions": {
      "target": "ESNext",
      "useDefineForClassFields": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "declaration": true,
      "resolveJsonModule": true,
      "isolatedModules": false,
      "esModuleInterop": true,
      "lib": ["ESNext"],
      "skipLibCheck": true,
      "paths": {
        "@/*": ["./src/*"]
      }
    } ,
    "include":["src/**/*.ts","src/**/*.d.ts", "native-types/*.ts","autox-native.d.ts"]
  }
  